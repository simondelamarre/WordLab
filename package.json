{
  "name": "@landscape/wordlab",
  "version": "1.0.7",
  "description": "Wordlab is a vector3D text classifier that allow you to sort indexs by distance writted for French",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "prepublish": "tsc",
  "files": [
    "lib/**/*"
  ],
  "directories": {
    "lib": "lib",
    "test": "test",
    "bin": "bin"
  },
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "npx tsc",
    "demo": "node demo/demo.js",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "browserify": "browserify lib/index.js -o dist/bundle2.js",
    "brow": "browserify src/index.ts -p [ tsify --noImplicitAny ] > dist/bundle3.js",
    "browser": "browserify lib/index.js > dist/bundle.js",
    "minify": "jsmin -o dist/bundle.min.js dist/bundle.js",
    "uglify": "uglifyjs --compress --mangle --output dist/bundle.min.js lib/bundle.js",
    "watch": "watchify dist/bundle.js -o dist/bundle.js -v",
    "webpack": "npx webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simondelamarre/WordLab.git"
  },
  "keywords": [
    "WordLab",
    "KNN",
    "classifier",
    "text",
    "vector3D",
    "nearest",
    "neighbors"
  ],
  "author": "Simon Delamarre",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/simondelamarre/WordLab/issues"
  },
  "homepage": "https://github.com/simondelamarre/WordLab#readme",
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^26.0.10",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^10.0.0",
    "browserify": "^16.5.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "jest": "^26.4.2",
    "jsmin": "^1.0.1",
    "prettier": "^2.1.1",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.3",
    "tsify": "^5.0.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.2",
    "uglify-js": "^3.10.4",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.11.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "es6-promise": "^4.2.8",
    "gsap": "^3.5.1",
    "gulp": "^4.0.2",
    "isomorphic-fetch": "^2.2.1",
    "reading-time": "^1.2.0",
    "three": "^0.120.1"
  }
}
